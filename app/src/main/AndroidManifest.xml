<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="societymanagementsystem.manage.management.com.cipla">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@drawable/cipla_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="societymanagementsystem.manage.management.com.cipla.MainActivity"
            android:excludeFromRecents="true"
            android:hardwareAccelerated="false"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="societymanagementsystem.society.management.com.cipla.MainActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="societymanagementsystem.manage.management.com.cipla.Splash_Screen_Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="societymanagementsystem.manage.management.com.cipla.user" />
        <activity android:name="societymanagementsystem.manage.management.com.cipla.feedback"></activity>
    </application>

</manifest>